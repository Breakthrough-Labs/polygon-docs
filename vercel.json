{
  "installCommand": "pip3 install virtualenv && virtualenv venv; source venv/bin/activate && pip3 install -r requirements.txt",
  "buildCommand": "source venv/bin/activate && mkdocs build",
  "outputDirectory": "site",
  "trailingSlash": true,
  "rewrites": [{
    "source": "/docsbot/docsbot.css",
    "destination": "https://cookbook-docsbot-staging.vercel.app/docsbot.css"
  }, {
    "source": "/docsbot/docsbot.min.js",
    "destination": "https://cookbook-docsbot-staging.vercel.app/docsbot.min.js"
  }]
}